{
    "name": "cryptomkt/cryptomkt-php",
    "description": "Crypto Market PHP Client",
    "keywords": [ "ethereum", "cryptomkt", "stellar", "php", "client", "bitcoin", "lumen", "cryptocurrencies", "blockchain"],
    "homepage": "https://www.cryptomkt.com",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": { "Cryptomkt\\Exchange\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Cryptomkt\\Exchange\\Tests\\": "tests/" }
    },
    "require": {
        "php": ">=5.6.0",
	"guzzlehttp/guzzle": "~4.0|~5.0",
        "psr/http-message": "^1.0",
        "psr/log": "^1.0",
        "guzzlehttp/psr7": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "config": {
        "bin-dir": "bin/"
    },
    "extra": {
        "branch-alias": {
            "master": "0.1.0"
        }
    }
}
