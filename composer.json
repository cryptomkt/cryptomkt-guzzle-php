{
    "name": "cryptomkt/cryptomkt",
    "description": "Cryptomkt API library",
    "keywords": [ "ethereum", "cryptomkt" ],
    "homepage": "http://www.cryptomkt.com",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": { "Cryptomkt\\Wallet\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Cryptomkt\\Wallet\\Tests\\": "tests/" }
    },
    "require": {
        "php": ">=5.6.0",
        "guzzlehttp/guzzle": "^6.0",
        "psr/http-message": "^1.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "config": {
        "bin-dir": "bin/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
